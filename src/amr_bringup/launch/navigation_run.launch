    <!--include file="$(find robot_pose_ekf)/launch/robot_pose_ekf.launch"/-->
    <include file="$(find set_navigation)/launch/navigation_rviz.launch"/>             <!--已知地图的自主导航-->

    <!-- 不用amcl的时候设置一个/odom与/map之间的静态坐标变换 -->
    <!--node pkg="tf" type="static_transform_publisher" name="map_odom_broadcaster" args="0 0 0 0 0 0 /map /odom 50" /-->

